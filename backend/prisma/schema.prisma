// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Form1 {
  id                Int      @default(autoincrement()) @id
  createdAt         DateTime @default(now())
  handsomeRating    Int
}

model Form2 {
  id                Int      @default(autoincrement()) @id
  createdAt         DateTime @default(now())
  hairDesc          String
}